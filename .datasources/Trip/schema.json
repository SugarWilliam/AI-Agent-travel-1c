{
  "name": "Trip",
  "title": "旅行计划",
  "schemas": [
    {
      "name": "title",
      "title": "计划标题",
      "type": "String"
    },
    {
      "name": "destination",
      "title": "目的地",
      "type": "String"
    },
    {
      "name": "startDate",
      "title": "出发日期",
      "type": "Date",
      "format": "date"
    },
    {
      "name": "endDate",
      "title": "返回日期",
      "type": "Date",
      "format": "date"
    },
    {
      "name": "days",
      "title": "天数",
      "type": "Number"
    },
    {
      "name": "budget",
      "title": "预算",
      "type": "Number"
    },
    {
      "name": "travelers",
      "title": "出行人数",
      "type": "Number"
    },
    {
      "name": "description",
      "title": "行程简介",
      "type": "MultiLineString"
    },
    {
      "name": "coverImage",
      "title": "封面图片",
      "type": "Image"
    },
    {
      "name": "status",
      "title": "计划状态",
      "type": "String"
    },
    {
      "name": "itinerary",
      "title": "行程安排",
      "type": "Array",
      "items": {
        "type": "Object"
      }
    },
    {
      "name": "weather",
      "title": "天气预报",
      "type": "Array",
      "items": {
        "type": "Object"
      }
    },
    {
      "name": "guide",
      "title": "旅游攻略",
      "type": "Object"
    },
    {
      "name": "photoTips",
      "title": "拍照指导",
      "type": "Object"
    },
    {
      "name": "outfitTips",
      "title": "穿搭建议",
      "type": "Object"
    },
    {
      "name": "preferences",
      "title": "旅行偏好",
      "type": "MultiLineString"
    }
  ]
}